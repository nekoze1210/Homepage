{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:import/warnings",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "no-restricted-imports": [
      "error",
      {
        "patterns": [
          "./",
          "../"
        ]
      }
    ]
  },
  "ignorePatterns": ["next-sitemap.js"]
}
